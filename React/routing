import React from "react";
import { BrowserRouter as Router, Route, Routes, Link } from "react-router-dom";

// ElectronicsGoods Component
function ElectronicsGoods() {
  return <h2>Electronics Goods List</h2>;
}

// Home Component
function Home() {
  return (
    <div>
      <h1>Categories</h1>
      <ul>
        <li>
          <Link to="/electronics">Electronics</Link>
        </li>
        <li>Personal</li>
        <li>Food</li>
      </ul>
    </div>
  );
}

// App Component with Routing
function App() {
  return (
    <Router>
      <Routes>
        <Route path="/" element={<Home />} />
        <Route path="/electronics" element={<ElectronicsGoods />} />
      </Routes>
    </Router>
  );
}

export default App;
